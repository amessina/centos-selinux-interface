+        systemd_read_fifo_file_passwd_run($1)
+ allow $1 hostapd_unit_file_t:file read_file_perms;
+ allow $1 hostapd_unit_file_t:service manage_service_perms;
+
+ ps_process_pattern($1, hostapd_t)
